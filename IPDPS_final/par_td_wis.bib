@article{Hicks2005Cycle,
  author = {Illya Hicks},
  title = "Planar Branch Decompositions {II}: The Cycle Method",
  journal = {INFORMS Journal on Computing},
  volume = "17",
  issue = "4",
  pages = "413--421",
  year = "2005",
  comment={\\\\Describes an algorithm for computing optimal branch decomposition for planar graphs.  Claims
  to be an improvement over an edge-contraction based method proposed by Seymour-Thomas and is called
  the cycle method.  Tree is constructed a la Cook-Seymour and the same notions of extendibility are
  discussed.  One of the key points here is that extendibility can be proven for planar graphs in much
  wider context - the ratcatcher algorithm can be used to check whether or not a decomposition is extendible,
  whereas in the general, non-planar case, once we make the first "non-safe" splitting of a partial
  decomposition, we have no way of knowing the extendibility any more.  As in the ratcatcher paper,
  the medial graph is used - running times are smaller, but for some reason he doesn't run the algorithm
  on the same sets of graphs... }
  }

@article{Hicks2005Rat,
  author = {Illya Hicks},
  title = "Planar Branch Decompositions {I}: The Ratcatcher",
  journal = {INFORMS Journal on Computing},
  volume = "17",
  issue = "4",
  pages = "402--412",
  year = "2005",
  comment={\\\\Describes an algorithm for computing optimal branch decomposition for planar graphs.  Algorithm
  requires one to construct the medial graph and then run the ratcatcher procedure.  Complexity of the first
  algorithm is $O(e^2)$ (where $G$ has $e$ edges) but memory issues apparently prevent it from being tractable for more than a few
  hundred edges.  He proposes two alternatives, comprat and memrat that have higher $O(e^3)$ complexity
  but lower memory requirements.  The memrat was the only one to not run out of memory (1 GB available) on a 15K node graph (brd14051) which
  had branchwidth 68 - runtime was about 1 week. }
  }

@article{BodlaenderKoster2008,
