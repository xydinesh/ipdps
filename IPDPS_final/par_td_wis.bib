@article{MillerTengThurstonVavasis1997,
  author    = {Gary Miller and Shang-Hua Teng and William Thurston and Stephen Vavsis},
  title     = {Separators for Sphere-Packings and Nearest Neighbor Graphs},
  journal   = {Journal of the ACM},
  volume    = {44},
  number    = {1},
  year      = {1997},
  pages     = {1-29},
  comment = {\\\\This paper proves the existence of sphere separators for k-ply systems of balls in dimension d satisfying bounds on the size of the intersection and
cardinalities of the partition components. The relevant implication for graph decomposition is the implication that every k-nearest neighbor-graph on n points in d dimensions
has a separator of order O($k^{1/d}n^{1-1/d}$). This generalizes the planar separator theorem of Lipton \& Tarjan to higher dimensions (this theorem is used in the proof of the
graph minors theorem). Gives linear serial and O($\log n$) parallel algorithms for finding a small separator of graphs embedded in d dimensions satisfying the appropriate
geometric condition.},
}

