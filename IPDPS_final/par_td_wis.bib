@article{Amir2008,
  author    = {Eyal Amir},
  title     = {Approximation Algorithms for Treewidth},
  journal   = {Algorithmica},
  volume    = {},
  number    = {},
  year      = {2008},
  pages     = {in press},
  comment = {\\\\Presents 4 algorithms for treewidth. The first is factor-O($\log$ OPT) from the optimal,
where OPT is the treewidth of the given graph and runs in time O($n^4 k \log k$) with
$n$ the number of nodes and $k$ = OPT, the treewidth. The other three offer factor-4, factor-4.5 and factor-3.67
approximations with running times O($2^{4.38k}n^2k$), O($2^{3k} n^2 k^{3/2}$) and O($2^k poly(n)$), respectively.
Code is available at reason.cs.uiuc.edu/eyal/decomp.}
}

@inproceedings{FeigeHajiaghayiLee2005,
  author    = {Uriel Feige and MohammedTaghi Hajiaghayi and James Lee},
  title     = {Improved Approximation Algorithms for Minimum-Weight Vertex Separators},
  booktitle   = {Proceedings of 37th STOC},
  year      = {2005},
  pages     = {563--572},
  comment = {\\\\There are numerous theoretical results in this paper regarding min-ratio vertex cuts \&  max-flow/min-cut theorems. These imply an
O($\sqrt{\log n}$) pseudo-approximation for balanced vertex separators in general graphs with an approximation ratio of O($\log$ OPT). Applied to
treewidth, we get that for a graph with treewidth $k$, an algorithm that finds a tree decomposition of width at most O($k\sqrt{\log k}$). For graphs with
a fixed excluded minor (includes all bounded genus graphs), this gives a constant factor approximation to the treewidth. These are direct improvements to the
algorithms of Amir2008.},
}

